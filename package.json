{
  "name": "html-css--assessment",
  "version": "1.0.0",
  "description": "HTML & CSS assessment",
  "scripts": {
    "watch": "node-sass sass/global.scss css/style.css -w",
    "compile": "node-sass sass/global.scss css/style.css",
    "prefix": "postcss css/style.css --use autoprefixer -o css/style.css",
    "compress": "node-sass css/style.css css/style.css --output-style compressed",
    "build": "npm-run-all compile prefix compress"
  },
  "repository": "",
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^9.0.1",
    "node-sass": "^4.9.0",
    "npm-run-all": "^4.1.3",
    "postcss-cli": "^6.0.0"
  }
}
